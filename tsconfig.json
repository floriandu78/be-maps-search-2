{
  "compilerOptions": {
    // "types": ["node","jest"],
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "rootDir": "./src",    

    "skipLibCheck": true, // Add this line
    "forceConsistentCasingInFileNames": true // Optional but recommended
  },
  "include": ["src/**/*", "tests/**/*"],
  "exclude": ["node_modules", "dist"]
}